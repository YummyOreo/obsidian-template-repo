{
  "database": "Zotero",
  "noteImportFolder": "Literature",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "APA Inline",
      "format": "formatted-citation",
      "cslStyle": "apa"
    },
    {
      "name": "APA",
      "format": "formatted-bibliography",
      "cslStyle": "apa",
      "brackets": false
    }
  ],
  "exportFormats": [
    {
      "name": "Literature Note",
      "outputPathTemplate": "Literature/{{citekey}}.md",
      "imageOutputPathTemplate": "{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "Templates/Literature Template.md"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "all-imported-notes",
  "shouldConcat": true
}